{
    "name": "documentrepo",
    "version": "1.0.0",
    "description": "DocumentRepo",
    "main": "server.js",
    "scripts": {
        "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
        "start:prod": "node server.js",
        "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
        "client": "cd client && npm run start",
        "seed": "node scripts/seedDB.js",
        "install": "cd client && npm install",
        "build": "cd client && npm run build",
        "heroku-postbuild": "npm run build"
    },
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@google-cloud/datastore": "^4.0.0",
        "@google-cloud/storage": "^1.7.0",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.18.3",
        "concurrently": "^4.0.1",
        "cors": "^2.8.5",
        "express": "^4.16.4",
        "is-empty": "^1.2.0",
        "jsonwebtoken": "^8.3.0",
        "lodash": "^4.17.21",
        "mongoose": "^5.3.11",
        "multer": "^1.4.2",
        "nconf": "^0.10.0",
        "passport": "^0.4.0",
        "passport-jwt": "^4.0.0",
        "prompt": "^1.0.0",
        "pug": "^2.0.3",
        "uglify-js": "^3.4.9",
        "validator": "^10.9.0",
        "if-env": "^1.0.4"
    }
}